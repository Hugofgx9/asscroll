  "bugs": {
    "url": "https://github.com/ashthornton-gc/asscroll/issues"
  },
  "homepage": "https://github.com/ashthornton-gc/asscroll#readme",
  "devDependencies": {
    "@unseenco/e": "^1.3.6"
  }
}
